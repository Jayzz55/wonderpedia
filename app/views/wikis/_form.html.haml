=form_for wiki do |f|
  .form-group
    =f.label :title, 'title'
    =f.text_field :title, class: 'form-control', placeholder: "Enter wiki title"
  .form-group
    =f.label :body, 'wiki'
    =f.text_area :body, rows: 8, class: 'form-control', placeholder: "Enter wiki description"
  =f.submit "Save", class: 'btn btn-success'